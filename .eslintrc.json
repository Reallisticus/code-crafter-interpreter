{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["xo", "plugin:@typescript-eslint/recommended", "prettier"],
  "overrides": [
    {
      "extends": ["xo-typescript"],
      "files": ["*.ts", "*.tsx"]
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
